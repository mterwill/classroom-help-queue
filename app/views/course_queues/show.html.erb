<%= render 'shared/breadcrumbs', trail: [
    { title: @course_queue.course.name, href: url_for(@course_queue.course) },
    @course_queue.name
] %>

<div id="app" class="sixteen wide column"></div>

<script type="text/babel">
    ReactDOM.render(
      <CourseQueue
          courseName={"<%= j @course_queue.course.name.html_safe %>"}
          queueName={"<%= j @course_queue.name.html_safe %>"}
          queueLoc={"<%= j @course_queue.location.html_safe %>"}
          currentUserId={<%= current_user.id %>}
          courseGroupId={<%= @course_group_id %>}
          groupMode={<%= @course_queue.group_mode.to_s %>}
          exclusiveMode={<%= @course_queue.exclusive.to_s %>}
          anonymousMode={<%= @course_queue.anonymous.to_s %>}
          instructor={<%= @is_instructor %>}
          id={<%= @course_queue.id %>}
      />,
      document.getElementById('app')
    );
</script>
